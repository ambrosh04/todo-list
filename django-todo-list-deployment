apiVersion: apps/v1
kind: Deployment
metadata:
  name: django-todo-list
spec:
  replicas: 2
  selector:
    matchLabels:
      app: django-todo-list
  template:
    metadata:
      labels:
        app: django-todo-list
    spec:
      containers:
      - name: django-todo-list
        image: public.ecr.aws/l6s9i6b7/todo-list:latest
        ports:
        - containerPort: 8000
